{
  "name": "positron-components",
  "version": "1.6.0",
  "repository": "github:profiidev/positron_components",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./backend": {
      "types": "./dist/backend/index.d.ts",
      "svelte": "./dist/backend/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "svelte": "./dist/components/index.js"
    },
    "./components/form": {
      "types": "./dist/components/form/index.d.ts",
      "svelte": "./dist/components/form/index.js"
    },
    "./components/nav": {
      "types": "./dist/components/nav/index.d.ts",
      "svelte": "./dist/components/nav/index.js"
    },
    "./components/table": {
      "types": "./dist/components/table/index.d.ts",
      "svelte": "./dist/components/table/index.js"
    },
    "./components/ui": {
      "types": "./dist/components/ui/index.d.ts",
      "svelte": "./dist/components/ui/index.js"
    },
    "./components/ui-extra": {
      "types": "./dist/components/ui-extra/index.d.ts",
      "svelte": "./dist/components/ui-extra/index.js"
    },
    "./components/util": {
      "types": "./dist/components/util/index.d.ts",
      "svelte": "./dist/components/util/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "svelte": "./dist/hooks/index.js"
    },
    "./util": {
      "types": "./dist/util/index.d.ts",
      "svelte": "./dist/util/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "svelte": "./dist/utils.js"
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "vite build && npm run prepack",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "sync": "svelte-kit sync"
  },
  "devDependencies": {
    "@emoji-mart/data": "^1.2.1",
    "@internationalized/date": "3.9.0",
    "@lucide/svelte": "^0.544.0",
    "@shikijs/langs": "^3.13.0",
    "@shikijs/themes": "^3.13.0",
    "@sveltejs/adapter-auto": "6.1.0",
    "@sveltejs/kit": "2.43.4",
    "@sveltejs/package": "2.5.4",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.13",
    "@tanstack/table-core": "8.21.3",
    "@types/luxon": "3.7.1",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "bits-ui": "^2.11.3",
    "clsx": "^2.1.1",
    "country-flag-icons": "^1.5.21",
    "embla-carousel-svelte": "^8.6.0",
    "formsnap": "2.0.1",
    "isomorphic-dompurify": "^2.28.0",
    "layerchart": "^2.0.0-next.38",
    "luxon": "3.7.2",
    "mode-watcher": "^1.1.0",
    "package-manager-detector": "^1.3.0",
    "paneforge": "^1.0.2",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "publint": "0.3.13",
    "runed": "^0.34.0",
    "shiki": "3.13.0",
    "svelte": "5.39.6",
    "svelte-check": "4.3.2",
    "svelte-easy-crop": "^5.0.0",
    "svelte-sonner": "^1.0.5",
    "svelte-tel-input": "^3.6.0",
    "svelte-toolbelt": "^0.10.5",
    "sveltekit-superforms": "2.27.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "4.1.13",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.2",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "7.1.7",
    "yeezy-dates": "^1.0.1",
    "zod": "4.1.11"
  }
}
