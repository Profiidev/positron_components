{
  "name": "positron-components",
  "version": "1.2.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "sync": "svelte-kit sync",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./backend": {
      "types": "./dist/backend/index.d.ts",
      "svelte": "./dist/backend/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "svelte": "./dist/components/index.js"
    },
    "./components/form": {
      "types": "./dist/components/form/index.d.ts",
      "svelte": "./dist/components/form/index.js"
    },
    "./components/nav": {
      "types": "./dist/components/nav/index.d.ts",
      "svelte": "./dist/components/nav/index.js"
    },
    "./components/table": {
      "types": "./dist/components/table/index.d.ts",
      "svelte": "./dist/components/table/index.js"
    },
    "./components/ui": {
      "types": "./dist/components/ui/index.d.ts",
      "svelte": "./dist/components/ui/index.js"
    },
    "./components/util": {
      "types": "./dist/components/util/index.d.ts",
      "svelte": "./dist/components/util/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "svelte": "./dist/hooks/index.js"
    },
    "./util": {
      "types": "./dist/util/index.d.ts",
      "svelte": "./dist/util/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "svelte": "./dist/utils.js"
    }
  },
  "dependencies": {
    "@internationalized/date": "^3.7.0",
    "@sveltejs/kit": "^2.20.6",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/table-core": "^8.21.2",
    "@types/luxon": "^3.6.2",
    "clsx": "^2.1.1",
    "formsnap": "^2.0.0",
    "lucide-svelte": "^0.487.0",
    "luxon": "^3.6.1",
    "mode-watcher": "^0.5.1",
    "svelte": "^5.25.8",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.24.1",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/package": "^2.0.0",
    "bits-ui": "^1.3.19",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "publint": "^0.3.10",
    "svelte": "^5.25.8",
    "svelte-check": "^4.1.5",
    "typescript": "^5.8.3",
    "vite": "^6.2.6"
  }
}
