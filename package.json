{
  "name": "positron-components",
  "version": "1.4.0",
  "repository": "github:profiidev/positron_components",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "sync": "svelte-kit sync",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./backend": {
      "types": "./dist/backend/index.d.ts",
      "svelte": "./dist/backend/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "svelte": "./dist/components/index.js"
    },
    "./components/form": {
      "types": "./dist/components/form/index.d.ts",
      "svelte": "./dist/components/form/index.js"
    },
    "./components/nav": {
      "types": "./dist/components/nav/index.d.ts",
      "svelte": "./dist/components/nav/index.js"
    },
    "./components/table": {
      "types": "./dist/components/table/index.d.ts",
      "svelte": "./dist/components/table/index.js"
    },
    "./components/ui": {
      "types": "./dist/components/ui/index.d.ts",
      "svelte": "./dist/components/ui/index.js"
    },
    "./components/util": {
      "types": "./dist/components/util/index.d.ts",
      "svelte": "./dist/components/util/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "svelte": "./dist/hooks/index.js"
    },
    "./util": {
      "types": "./dist/util/index.d.ts",
      "svelte": "./dist/util/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "svelte": "./dist/utils.js"
    }
  },
  "dependencies": {
    "@internationalized/date": "3.8.2",
    "@lucide/svelte": "0.516.0",
    "@sveltejs/kit": "2.21.5",
    "@sveltejs/vite-plugin-svelte": "5.1.0",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.10",
    "@tanstack/table-core": "8.21.3",
    "@types/luxon": "3.6.2",
    "bits-ui": "2.8.0",
    "clsx": "2.1.1",
    "embla-carousel-svelte": "^8.6.0",
    "formsnap": "2.0.1",
    "layerchart": "^2.0.0-next.10",
    "luxon": "3.6.1",
    "mode-watcher": "^1.0.8",
    "paneforge": "^1.0.0-next.5",
    "svelte": "5.34.3",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "2.27.0",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "1.0.0",
    "tailwindcss": "4.1.10",
    "tw-animate-css": "^1.3.0",
    "vaul-svelte": "^1.0.0-next.7",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "6.0.1",
    "@sveltejs/package": "2.3.11",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.4.0",
    "prettier-plugin-tailwindcss": "0.6.12",
    "publint": "0.3.12",
    "svelte": "5.34.3",
    "svelte-check": "4.2.1",
    "typescript": "5.8.3",
    "vite": "6.3.5"
  }
}
